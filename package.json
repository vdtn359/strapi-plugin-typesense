{
  "name": "strapi-plugin-typesense",
  "version": "1.8.0",
  "description": "Index articles configured to Typesense",
  "keywords": [
    "strapi",
    "plugin",
    "typesense"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdtn359/strapi-plugin-typesense.git"
  },
  "license": "MIT",
  "files": [
    "admin/**/*.ts",
    "admin/**/*.js",
    "admin/**/*.tsx",
    "admin/**/*.json",
    "dist/server/**/*.js",
    "dist/utils/**/*.js",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.server.json",
    "develop": "tsc -p tsconfig.server.json -w",
    "format": "prettier --write .",
    "lint": "run-p prettier eslint",
    "prettier": "prettier --check .",
    "eslint": "eslint --ignore-path .eslintignore .",
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "test:ci": "npm run test --ci",
    "prepare": "husky",
    "release": "release-it --only-version"
  },
  "dependencies": {
    "@strapi/admin": "^4.25.8",
    "@strapi/strapi": "^4.25.1",
    "@strapi/utils": "^4.25.8",
    "typesense": "^1.8.2",
    "deep-reduce": "^1.0.5",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@strapi/typescript-utils": "^4.25.8",
    "@types/jest": "^29.5.12",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "commitlint": "^19.4.0",
    "eslint": "^8.57.0",
    "eslint-config": "^0.3.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "release-it": "^17.6.0",
    "styled-components": "^5.3.6",
    "ts-jest": "^29.2.5",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.25.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "name": "strapi-typesense",
    "description": "Index articles configured to Typesense",
    "kind": "plugin",
    "icon": "plug"
  }
}
